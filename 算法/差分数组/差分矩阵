```cpp
#include<bits/stdc++.h>
using namespace std;
#define int long long
const int N = 1e3 +10;
int a[N][N] = { 0 };
int b[N][N] = { 0 }; 
int n, m,q;
void ins(int x1, int y1, int x2, int y2, int c) {
	b[x1][y1] += c;
	b[x2+1][y1] -= c;
	b[x1][y2 + 1] -= c;
	b[x2 + 1][y2 + 1] -= c;
}
signed main() {
	cin >> n >> m >> q;
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= m; j++)cin >> a[i][j];
	}
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= m; j++) {
			ins(i, j, i, j, a[i][j]);
		}
	}
	while (q--) {
		int x1, y1, x2, y2,c;
		cin >> x1 >> y1 >> x2 >> y2>>c;
		ins(x1, y1, x2, y2, c);
	}
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= m; j++) {
			b[i][j] = b[i - 1][j] + b[i][j - 1] - b[i - 1][j - 1] + b[i][j];
		}
	}
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= m; j++) {
			cout << b[i][j] << ' ';
		}
		cout << '\n';
	}
	
	return 0;
}
```